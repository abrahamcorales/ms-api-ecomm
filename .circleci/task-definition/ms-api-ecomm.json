{
    "containerDefinitions": [
        {
            "name": "ms-api-ecomm",
            "image": "780183661990.dkr.ecr.us-east-1.amazonaws.com/ms-api-ecomm",
            "cpu": 0,
            "memory": 250,
            "memoryReservation": 200,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "dockerLabels": {
                "traefik.frontend.rule": "PathPrefix:/api",
                "traefik.enable": "true",
                "traefik.backend": "ms-api-comm",
                "traefik.protocol": "http"
            },
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": []
          }
        ],
        "family": "ms-api-ecomm"
    }